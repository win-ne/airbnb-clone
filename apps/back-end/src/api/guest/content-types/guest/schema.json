{
  "kind": "collectionType",
  "collectionName": "guests",
  "info": {
    "singularName": "guest",
    "pluralName": "guests",
    "displayName": "Guest"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "phoneNumber": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "guest"
    }
  }
}
